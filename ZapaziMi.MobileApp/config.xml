<?xml version="1.0" encoding="utf-8"?>
<widget xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps" id="io.cordova.Diplomna" version="1.0.0.1" xmlns="http://www.w3.org/ns/widgets">
	<name>Запази ми</name>
	<description>Моята дипломна работа</description>
	<author href="http://cordova.io" email="lachezar1990@abv.bg">Лъчезар Спасов</author>
	<content src="index.html" />
	<access origin="*" />
	<access origin="http://*" />
	<access origin="mailto:*" launch-external="yes" />
	<access origin="tel:*" launch-external="yes" />
	<preference name="SplashScreen" value="screen" />
	<preference name="windows-target-version" value="8.0" />
	<preference name="windows-phone-target-version" value="8.1" />
	<preference name="Fullscreen" value="true" />
	<preference name="Orientation" value="portrait" />
	<vs:plugin name="org.apache.cordova.globalization" version="0.2.8" />
	<vs:plugin name="org.apache.cordova.inappbrowser" version="0.5.2" />
	<vs:plugin name="nl.x-services.plugins.calendar" version="4.3.6-dev" />
	<vs:plugin name="com.ionic.keyboard" version="1.0.4" />
	<vs:plugin name="cordova-plugin-network-information" version="1.0.1-dev" />
	<vs:platformSpecificValues />
</widget>